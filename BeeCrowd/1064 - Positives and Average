#include<stdio.h>
int main(){
    double n[6],sum=0;
    int count=0,i;
    for(i=0;i<6;i++){
        scanf("%lf",&n[i]);
    }

    for(i=0;i<6;i++){

       if(n[i]>0){
        sum +=n[i];
        count++;
       }
    }
    printf("%d valores positivos\n",count);
    printf("%.1lf\n",sum/count);
    return 0;
}
