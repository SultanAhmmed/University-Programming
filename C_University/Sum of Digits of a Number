// Online C compiler to run C program online
#include <stdio.h>

int main() {
   
   int num,num1;
   int total = 0;
   
   printf("Enter an Number: ");
   scanf("%d",&num);
   
   while(num!=0){
       num1= num%10;
       total = total + num1;
       num /=10;
   }
   printf("Sum of the Number's Digits: %d\n",total);
   
    return 0;
}
