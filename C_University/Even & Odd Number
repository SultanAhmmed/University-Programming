#include <stdio.h>

int main()
{
    int number=1,odd=0,even=0;
    while(number<=100)
    {
        if(number%2==0)
        {
            printf("Even Number: %d\n",number);
            even++;
        }
        else
        {
            printf("Odd Number: %d\n",number);
            odd++;
        }
        number++;
    }
    printf("Total Even Number:%d\n",even);
    printf("Total Odd Number: %d\n",odd);

    return 0;
}
