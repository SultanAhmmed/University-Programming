#include<stdio.h>
int main(){

    //Creating variables
    int notes,remanider;

    printf("Enter Notes: ");
    scanf("%d",&notes);

    printf("500: %d\n",notes/500);
    remanider = notes%500;
    
    printf("100: %d\n",remanider/100);
    remanider = remanider % 100;
    
    printf("50: %d\n",remanider/50);
    remanider = remanider%50;
    
    printf("20: %d\n", remanider/20);
    remanider = remanider %20;
    
    printf("10: %d\n",remanider/10);
    remanider = remanider%10;
    
    printf("5: %d\n",remanider/5);
    remanider = remanider%5;
    
    printf("2: %d\n",remanider/2);
    remanider= remanider%2;
    
    printf("1: %d\n",remanider/1);
    
    return 0;
}
