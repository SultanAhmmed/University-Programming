#include<stdio.h>
#define TRIANGLE_ANGLE 180
int main(){

    //Creating variables
    int angle1,angle2,angle3;
    int total;
    printf("Enter Angles: ");
    scanf("%d %d %d",&angle1,&angle2,&angle3);

    total=angle1+angle2+angle3;

    if(total==TRIANGLE_ANGLE){
        printf("The Triangle is valid\n");
    }
    else{
        printf("The Triangle is not valid\n");
    }
    return 0;
}
