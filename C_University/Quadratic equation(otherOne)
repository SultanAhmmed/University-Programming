#include<stdio.h>
int main()
{
    double D,a,b,c,x1,x2;

    printf("Enter the Value of(a,b,c): ");
    scanf("%lf %lf %lf",&a,&b,&c);

    D = b*b-4*a*c;
    x1 = (-b - D)/(2*a);
    x2 = (-b+ D)/(2*a);
    printf("The Solution of x1: %lf\n",x1);
    printf("The Solution of x2: %lf\n",x2);

    return 0;
}
