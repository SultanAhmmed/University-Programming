// Online C compiler to run C program online
#include <stdio.h>

int main() {
   
   int num,num1;
   int total = 1;
   
   printf("Enter a Number: ");
   scanf("%d",&num);
   
   while(num!=0){
       num1= num%10;
       total = total * num1;
       num /=10;
   }
   printf("Product of digits of a Number: %d\n",total);
   
    return 0;
}
