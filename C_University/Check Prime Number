#include<stdio.h>
int main()
{
    int num,count= 0;
    printf("Enter a Number: ");
    scanf("%d",&num);
    int i;
    for(i=2;i<num;i++){
        if(num%i==0){
            count++;
            break;
        }
    }
    if(count==0){
        printf("Prime");
    }
    else{
        printf("Not Prime");
    }
    return 0;
}
